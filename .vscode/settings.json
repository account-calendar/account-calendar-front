{
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.removeUnusedImports": "explicit"
  },
  "javascript.preferences.importModuleSpecifier": "non-relative",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "editor.tabSize": 2,
  "tailwindCSS.classFunctions": ["clsx", "cn"],
  "tailwind-breakpoint.breakpoints": [
    {
      "breakpoint": "mobile",
      "decoration": {
        "color": "#84ffdc"
      }
    },
    {
      "breakpoint": "pc",
      "decoration": {
        "color": "#617df7"
      }
    },
    {
      "breakpoint": "hover",
      "decoration": {
        "color": "#9661f7"
      }
    },
    {
      "breakpoint": "disabled",
      "decoration": {
        "color": "#969696",
        "fontWeight": "bold"
      }
    },
    {
      "breakpoint": "focus",
      "decoration": {
        "color": "#f2ffae"
      }
    },
    {
      "breakpoint": "active",
      "decoration": {
        "color": "#ff8383"
      }
    },
    {
      "breakpoint": "after",
      "decoration": {
        "color": "#dcdcdc"
      }
    },
    {
      "breakpoint": "before",
      "decoration": {
        "color": "#afafaf"
      }
    }
  ]
}
